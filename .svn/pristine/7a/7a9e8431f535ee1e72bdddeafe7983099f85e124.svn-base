package rice.ec.vr;

import java.io.Serializable;

/**
 * 商户数据实体
 */
public class VideoInfoEntity implements Serializable {
    public String ID;    //资源ID
    public String Type;  //资源类型
    public String Code;  //资源编号
    public String Url;   //资源的URL
    public String Version;  //资源的当前版本
    public String Title;    //资源的标题
    public String Profile; //资源的简介
    public String Size;    //资源的大小
    public String ServerType; //资源服务端的类型
    public String Creator;    //资源的发布者
    public String Preview;    //资源的缩略图
    public String CreateTime;    //创建时间
}
